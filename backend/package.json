{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/functions/*.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "prestart": "npm run clean && npm run build",
    "start": "func start",
    "test": "echo \"No tests yet...\"",
    "azurite": "azurite --silent --location ./azurite --debug ./azurite/debug.log",
    "seed": "node scripts/clients-to-data-tables.js --auto && node scripts/upload-to-azurite.js",
    "dev": "concurrently \"npm:azurite\" \"wait-on http://127.0.0.1:10002/devstoreaccount1\" \"npm run seed\" \"func start\"",
    "azurite:clean": "rm -rf ./azurite"
  },
  "dependencies": {
    "@azure/data-tables": "^13.3.1",
    "@azure/functions": "^4.0.0",
    "@azure/storage-blob": "^12.10.0",
    "exceljs": "^4.4.0",
    "jsonwebtoken": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.18.6",
    "azure-functions-core-tools": "^4.x",
    "azurite": "^3.35.0",
    "concurrently": "^9.2.1",
    "cpy-cli": "^6.0.0",
    "rimraf": "^5.0.10",
    "typescript": "^4.0.0",
    "wait-on": "^9.0.0"
  }
}
